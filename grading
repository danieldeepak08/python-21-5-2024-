mark1 = int(input())
mark2 = int(input())
mark3 = int(input())

average_mark = (mark1 + mark2 + mark3) / 3

if average_mark >= 90:
    grade = 'A'
elif 80 <= average_mark < 90:
    grade = 'B'
elif 70 <= average_mark < 80:
    grade = 'C'
elif 60 <= average_mark < 70:
    grade = 'D'
elif 50 <= average_mark < 60:
    grade = 'E'
else:
    grade = 'F'

print("The grade is", grade)
